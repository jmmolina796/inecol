/* Variables */


/* Mixins */

totalCenterElement()
	left 50%
	position absolute
	top 50%
	transform translateX(-50%) translateY(-50%)
	

verticalCenterElement()
	position absolute
	top 50%
	transform translateY(-50%)
	
horizontalCenterElement()
	position absolute
	left 50%
	transform translateX(-50%)

/* Mixins */


/* Fonts */

@font-face
	font-family 'icomoon'
	src url('fonts/icomoon.eot?-619b11')
	src url('fonts/icomoon.eot?#iefix-619b11') format('embedded-opentype'),
		url('fonts/icomoon.woff?-619b11') format('woff'),
		url('fonts/icomoon.ttf?-619b11') format('truetype'),
		url('fonts/icomoon.svg?-619b11#icomoon') format('svg')
	font-weight normal
	font-style normal

/* Fonts */
	

/* Animations */

@keyframes sk-bouncedelay 
	0%, 80%, 100% 
		transform scale(0)
	40% 
		transform scale(1.0)

@keyframes sk-bounce 
	0%, 100% 
		transform scale(0.0)
	50%
		transform scale(1.0)

@keyframes loading 
	from 
		left 50%
		width 0
		z-index 100
	33.3333%
		left 0
		width 100%
		z-index 10
	to 
		left 0 
		width 100%

